{
    "name": "saeedpooyanfar/zarinpal",
    "type": "library",
    "description": "Zarinpal payment for Laravel Framework (with sandbox support)",
    "keywords": ["zarinpal", "payment", "laravel", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Pooyanfar",
            "email": "saeedp47@gmail.com",
            "homepage": "https://github.com/Saeed-Pooyanfar"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^7.4",
      "guzzlehttp/guzzle": "^7.0.1",
      "ext-json": "*",
      "ext-soap": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zarinpal\\ZarinpalServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Zarinpal\\": "src/"
        }
    }
